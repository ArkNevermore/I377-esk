<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<persistence xmlns="http://java.sun.com/xml/ns/persistence" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="2.0" 
	xsi:schemaLocation="http://java.sun.com/xml/ns/persistence http://java.sun.com/xml/ns/persistence/persistence_2_0.xsd">
<persistence-unit name="persistenceUnit" transaction-type="RESOURCE_LOCAL">
        <provider>org.hibernate.ejb.HibernatePersistence</provider>
        <class>entities.AdminAlluvus</class>
        <class>entities.Amet</class>
        <class>entities.AmetPiiripunkti</class>
        <class>entities.AmetVaeosa</class>
        <class>entities.Auaste</class>
        <class>entities.AuastmeMuutumine</class>
        <class>entities.IntsidendiLiik</class>
        <class>entities.Intsident</class>
        <class>entities.IsikuSeadusIntsidendi</class>
        <class>entities.IsikIntsidendi</class>
        <class>entities.Kodakondsus</class>
        <class>entities.KontaktiLiik</class>
        <class>entities.Objekt</class>
        <class>entities.ObjektiLiik</class>
        <class>entities.ObjektiSeadusIntsidendi</class>
        <class>entities.ObjektIntsidendi</class>
        <class>entities.OrgYksus</class>
        <class>entities.PiiriloiguHaldaja</class>
        <class>entities.Piiriloik</class>
        <class>entities.Piiripunkt</class>
        <class>entities.PiiripunktiAlluvus</class>
        <class>entities.PiiripunktiOrgYksus</class>
        <class>entities.Piiririkkuja</class>
        <class>entities.Piirivalvur</class>
        <class>entities.PiirivalvuriKontakt</class>
        <class>entities.PiirivalvuriSeadusIntsidendi</class>
        <class>entities.PiirivalvurIntsidendi</class>
        <class>entities.PiirivalvurPiiripunkti</class>
        <class>entities.PiirivalvurVaeosa</class>
        <class>entities.PiirivalvurVodikohal</class>
        <class>entities.RiigiAdminYksus</class>
        <class>entities.RiigiAdminYksuseLiik</class>
        <class>entities.Riik</class>
        <class>entities.Ruumiyksus</class>
        <class>entities.RuumiyksuseLiik</class>
        <class>entities.Seadus</class>
        <class>entities.SeadusePunkt</class>
        <class>entities.SeotudKontaktisik</class>
        <class>entities.SuguluseRolliLiik</class>
        <class>entities.Vaeosa</class>
        <class>entities.VaeosaAlluvus</class>
        <class>entities.Vahtkond</class>
        <class>entities.VahtkondIntsidendi</class>
        <class>entities.VahtkonnaLiige</class>
        <class>entities.VahtkonndPiiriloigul</class>
        <class>entities.VoimalikAlluvus</class>
        <class>entities.Voodikoht</class>
        <properties>
            <property name="hibernate.dialect" value="org.hibernate.dialect.HSQLDialect"/>
            <!-- value="create" to build a new database on each run; value="update" to modify an existing database; value="create-drop" means the same as "create" but also drops tables when Hibernate closes; value="validate" makes no changes to the database -->
            <property name="hibernate.hbm2ddl.auto" value="update"/>
            <property name="hibernate.ejb.naming_strategy" value="org.hibernate.cfg.ImprovedNamingStrategy"/>
            <property name="hibernate.connection.charSet" value="UTF-8"/>
            <!-- Uncomment the following two properties for JBoss only -->
            <!-- property name="hibernate.validator.apply_to_ddl" value="false" /-->
            <!-- property name="hibernate.validator.autoregister_listeners" value="false" /-->
        </properties>
    </persistence-unit>
</persistence>
